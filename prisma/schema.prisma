generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["jsonProtocol"]
}

datasource db {
    provider = "sqlite"
    // Using SQLite for mock fake data purpose
    url      = env("DATABASE_URL")
}


model User {
    id            String    @id @default(cuid())
    email         String?   @unique
    emailVerified DateTime?
    password      String
}
